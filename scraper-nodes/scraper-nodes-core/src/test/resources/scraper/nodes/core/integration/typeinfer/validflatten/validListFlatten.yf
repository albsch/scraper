name: mapJoin
graphs:
  start:
    - type: EchoNode
      puts:
        toFlatA: ["a","b"]
        toFlatB: ["c","d"]

    - type: FlattenListNode
      flatten: ["{toFlatA}", "{toFlatB}"]

    - type: AssertNode
      failOnError: true
      assertMap:
        toFlatA: ["a","b"]
        toFlatB: ["c","d"]
        output: ["a","b","c","d"]
